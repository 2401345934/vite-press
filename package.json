{
  "name": "@xiaomh/vitepress",
  "version": "1.0.8",
  "main": "index.js",
  "scripts": {
    "version": "zx bump.mjs",
    "start": "vitepress dev docs",
    "build": "npm run docs:build",
    "docs:dev": "vitepress dev docs",
    "commit": "npm run version && npm run changelog && git add . && git cz ",
    "push": "npm run version && npm run changelog && git add . && git cz && git ps",
    "changelog": "conventional-changelog -p custom-config -i docs/CHANGELOG/index.md -s -r 0",
    "docs:build": "vitepress build docs",
    "prepare": "husky install",
    "docs:serve": "vitepress serve docs"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@commitlint/config-conventional": "^17.1.0",
    "@types/node": "^18.11.8",
    "commitizen": "^4.2.5",
    "commitlint": "^17.1.2",
    "commitlint-config-cz": "^0.13.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "markdown-it": "^13.0.1",
    "prettier": "^2.7.1",
    "prompts": "^2.4.2",
    "vitepress": "^1.0.0-alpha.26",
    "vitepress-plugin-search": "^1.0.4-alpha.14",
    "zx": "^7.1.1"
  },
  "dependencies": {
    "@xiaomh/vue3-alan-vite-component": "^1.0.31",
    "blueimp-md5": "^2.19.0",
    "dayjs": "^1.11.6",
    "fast-glob": "^3.2.12",
    "gray-matter": "^4.0.3",
    "valine": "^1.5.1"
  }
}